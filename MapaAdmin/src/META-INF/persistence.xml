    <class>mapaadmin.dao.Funcionario</class>
    <class>mapaadmin.dao.TipoUso</class>
    <class>mapaadmin.dao.Ciudad</class>
    <class>mapaadmin.dao.ZonaInfo</class>
    <class>mapaadmin.dao.Departamento</class>
    <class>mapaadmin.dao.EdificioPiso</class>
    <class>mapaadmin.dao.EdificioInfo</class>
    <class>mapaadmin.dao.TipoEspacio</class>
    <class>mapaadmin.dao.EdificioEspacio</class>
    <class>mapaadmin.dao.TipoCargo</class>
    <class>mapaadmin.dao.TipoZona</class>
    <properties>
      <property name="toplink.jdbc.user" value="postgres"/>
      <property name="toplink.jdbc.password" value="ucentral2009"/>
      <property name="toplink.jdbc.url" value="jdbc:postgresql://localhost:5432/ucentral2"/>
      <property name="toplink.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="toplink.cache.size.default" value="50"/>
    </properties>
  </persistence-unit>
</persistence>
